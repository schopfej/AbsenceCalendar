<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">

	<managed-bean>
		<managed-bean-name>absencesTable</managed-bean-name>
		<managed-bean-class>ch.nbc.eac.backing.AbsencesTableSBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>timeRange</property-name>
			<value>#{absencesTimeRange}</value>
		</managed-property>
		<managed-property>
			<property-name>employeePool</property-name>
			<value>#{employeePool}</value>
		</managed-property>
		<managed-property>
			<property-name>user</property-name>
			<value>#{user}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>absencesTimeRange</managed-bean-name>
		<managed-bean-class>ch.nbc.eac.backing.AbsencesTimeRangeSBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>employeePoolApplication</managed-bean-name>
		<managed-bean-class>ch.nbc.eac.backing.EmployeePoolABean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>employeePool</managed-bean-name>
		<managed-bean-class>ch.nbc.eac.backing.EmployeePoolSBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>applicationData</property-name>
			<value>#{employeePoolApplication}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>selectedEmployees</managed-bean-name>
		<managed-bean-class>ch.nbc.eac.backing.SelectedEmployeeSBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>user</property-name>
			<value>#{user}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>user</managed-bean-name>
		<managed-bean-class>ch.nbc.eac.backing.UserSBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>day</managed-bean-name>
		<managed-bean-class>ch.nbc.eac.backing.DayRBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
</faces-config>
